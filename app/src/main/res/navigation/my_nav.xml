<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/coinListFragment">
    <fragment
        android:id="@+id/coinListFragment"
        android:name="com.example.cryptocurrency.presentation.fragment.coin_list.CoinListFragment"
        android:label="fragment_coin_list"
        tools:layout="@layout/fragment_coin_list" >
        <action
            android:id="@+id/action_coinListFragment_to_coinDetailFragment2"
            app:destination="@id/coinDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/coinDetailFragment"
        android:name="com.example.cryptocurrency.presentation.fragment.coin_detail.CoinDetailFragment"
        android:label="fragment_coin_detail"
        tools:layout="@layout/fragment_coin_detail" >
        <action
            android:id="@+id/action_coinDetailFragment_to_coinListFragment2"
            app:destination="@id/coinListFragment" />
        <argument android:name="coinItem"
            app:argType="com.example.cryptocurrency.domain.model.Coin" />
    </fragment>
</navigation>